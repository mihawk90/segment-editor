{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist/spa",
    "devUrl": "http://localhost:3111"
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "MIT",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Manage Intro, Outro and other media segments for your Jellyfin Server",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "Manage Jellyfin Media Segments",
    "linux": {
      "deb": {
        "depends": []
      },
      "appimage": {
        "bundleMediaFramework": true
      }
    }
  },
  "productName": "Jellyfin Segment Editor",
  "version": "../package.json",
  "identifier": "com.endrl.segment.editor",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 900,
        "resizable": true,
        "center": true,
        "title": "Jellyfin Segment Editor",
        "width": 1200
      }
    ],
    "security": {
      "csp": null
    }
  }
}
